      steps {
        echo "Test for Jenkinsfile"
      }
    }

    stage('stage2') {
          steps {
            echo "This Jenkins file is Working"
          }
    }
    
    stage('step3') {
      steps {
        echo "Now, Let us build a real CI-CD pipeline"
      }
    }

  }
}
